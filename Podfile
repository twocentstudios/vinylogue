source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '11.0'

inhibit_all_warnings!

def shared_pods
    pod 'AFNetworking', '1.3.3'
    pod 'ReactiveCocoa', '2.3.1'
    pod 'ViewUtils', '1.1.2'
    pod 'UIViewDrawRectBlock', '0.0.1'
    pod 'SDURLCache', '1.3.0'
    pod 'FlurrySDK', '5.4.0'
    pod 'CrashlyticsFramework', '2.2.4'
end

target 'vinylogue' do
    use_frameworks!
    shared_pods
end
